
CREATE TABLE com_logali_order (
  clientGender NVARCHAR(255),
  status INTEGER,
  Priority NVARCHAR(255)
);

CREATE TABLE com_logali_Emails (
  email_01 NCLOB,
  email_02 NCLOB,
  email_03_kind NVARCHAR(255),
  email_03_email NVARCHAR(255)
);

CREATE TABLE com_logali_Car (
  ID NVARCHAR(36) NOT NULL,
  name NVARCHAR(255),
  PRIMARY KEY(ID)
);

CREATE TABLE com_logali_Products (
  ID NVARCHAR(36) NOT NULL,
  Name NVARCHAR(255) NOT NULL,
  Description NVARCHAR(255),
  ImageUrl NVARCHAR(255),
  ReleaseDate DATETIME_TEXT DEFAULT CURRENT_TIMESTAMP,
  DiscontinuedDate DATETIME_TEXT,
  Price DECIMAL(16, 2),
  Height DECIMAL(16, 2),
  Width DECIMAL(16, 2),
  Depth DECIMAL(16, 2),
  Quantity DECIMAL(16, 2),
  Supplier_ID NVARCHAR(36),
  UnitOfMeasure_ID NVARCHAR(2),
  PriceCondition NVARCHAR(2),
  PriceDetermination NVARCHAR(3),
  PRIMARY KEY(ID)
);

CREATE TABLE com_logali_Suppliers (
  ID NVARCHAR(36) NOT NULL,
  Name NVARCHAR(255) NOT NULL,
  Address_Street NVARCHAR(255),
  Address_City NVARCHAR(255),
  Address_State NVARCHAR(2),
  Address_PostalCode NVARCHAR(5),
  Address_Country NVARCHAR(3),
  Email NVARCHAR(255),
  Phone NVARCHAR(255),
  Fax NVARCHAR(255),
  PRIMARY KEY(ID)
);

CREATE TABLE com_logali_Categories (
  ID NVARCHAR(1) NOT NULL,
  Name NVARCHAR(255),
  PRIMARY KEY(ID)
);

CREATE TABLE com_logali_StockAvailability (
  ID INTEGER NOT NULL,
  Description NVARCHAR(255),
  PRIMARY KEY(ID)
);

CREATE TABLE com_logali_Currencies (
  ID NVARCHAR(3) NOT NULL,
  Description NVARCHAR(255),
  PRIMARY KEY(ID)
);

CREATE TABLE com_logali_UnitOfMeasures (
  ID NVARCHAR(2) NOT NULL,
  Description NVARCHAR(255),
  PRIMARY KEY(ID)
);

CREATE TABLE com_logali_DimensionUnits (
  ID NVARCHAR(2) NOT NULL,
  Description NVARCHAR(255),
  PRIMARY KEY(ID)
);

CREATE TABLE com_logali_Months (
  ID NVARCHAR(2) NOT NULL,
  Description NVARCHAR(255),
  ShortDescription NVARCHAR(3),
  PRIMARY KEY(ID)
);

CREATE TABLE com_logali_ProductReview (
  ID NVARCHAR(36) NOT NULL,
  ToProduct_Id NVARCHAR(255),
  CreatedAt DATETIME_TEXT,
  Name NVARCHAR(255),
  Rating INTEGER,
  Comment NVARCHAR(255),
  PRIMARY KEY(ID)
);

CREATE TABLE com_logali_SalesData (
  ID NVARCHAR(36) NOT NULL,
  DeliveryDate DATETIME_TEXT,
  Revenue DECIMAL(16, 2),
  PRIMARY KEY(ID)
);

CREATE VIEW com_logali_SelfProducts AS SELECT
  Products_0.ID,
  Products_0.Name,
  Products_0.Description,
  Products_0.ImageUrl,
  Products_0.ReleaseDate,
  Products_0.DiscontinuedDate,
  Products_0.Price,
  Products_0.Height,
  Products_0.Width,
  Products_0.Depth,
  Products_0.Quantity,
  Products_0.Supplier_ID,
  Products_0.UnitOfMeasure_ID,
  Products_0.PriceCondition,
  Products_0.PriceDetermination
FROM com_logali_Products AS Products_0;

CREATE VIEW com_logali_SelfProducts1 AS SELECT
  Products_0.ID,
  Products_0.Name,
  Products_0.Description,
  Products_0.ImageUrl,
  Products_0.ReleaseDate,
  Products_0.DiscontinuedDate,
  Products_0.Price,
  Products_0.Height,
  Products_0.Width,
  Products_0.Depth,
  Products_0.Quantity,
  Products_0.Supplier_ID,
  Products_0.UnitOfMeasure_ID,
  Products_0.PriceCondition,
  Products_0.PriceDetermination
FROM com_logali_Products AS Products_0;

CREATE VIEW com_logali_SelfProducts2 AS SELECT
  Products_0.Name,
  Products_0.Price,
  Products_0.Quantity
FROM com_logali_Products AS Products_0;

CREATE VIEW com_logali_SelfProducts3 AS SELECT
  ProductReview_1.Rating,
  Products_0.Name,
  sum(Products_0.Price) AS TotalPrice
FROM (com_logali_Products AS Products_0 LEFT JOIN com_logali_ProductReview AS ProductReview_1 ON Products_0.Name = ProductReview_1.Name)
GROUP BY ProductReview_1.Rating, Products_0.Name
ORDER BY Rating;

CREATE VIEW com_logali_ProjProducts AS SELECT
  Products_0.ID,
  Products_0.Name,
  Products_0.Description,
  Products_0.ImageUrl,
  Products_0.ReleaseDate,
  Products_0.DiscontinuedDate,
  Products_0.Price,
  Products_0.Height,
  Products_0.Width,
  Products_0.Depth,
  Products_0.Quantity,
  Products_0.Supplier_ID,
  Products_0.UnitOfMeasure_ID,
  Products_0.PriceCondition,
  Products_0.PriceDetermination
FROM com_logali_Products AS Products_0;

CREATE VIEW com_logali_ProjProducts2 AS SELECT
  Products_0.ID,
  Products_0.Name,
  Products_0.Description,
  Products_0.ImageUrl,
  Products_0.ReleaseDate,
  Products_0.DiscontinuedDate,
  Products_0.Price,
  Products_0.Height,
  Products_0.Width,
  Products_0.Depth,
  Products_0.Quantity,
  Products_0.Supplier_ID,
  Products_0.UnitOfMeasure_ID,
  Products_0.PriceCondition,
  Products_0.PriceDetermination
FROM com_logali_Products AS Products_0;

CREATE VIEW com_logali_ProjProducts3 AS SELECT
  Products_0.ReleaseDate,
  Products_0.Name
FROM com_logali_Products AS Products_0;

